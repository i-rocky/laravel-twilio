{
  "name": "i-rocky/laravel-twilio",
  "description": "Twilio Fax, SMS, Call receiving and sending support along with dashboard for laravel",
  "version": "2.0.1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rocky",
      "email": "wpkpda@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rocky\\LaravelTwilio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rocky\\LaravelTwilio\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rocky\\LaravelTwilio\\Providers\\LaravelTwilioServiceProvider"
      ],
      "aliases": {
        "LaravelTwilio": "Rocky\\LaravelTwilio\\LaravelTwilio"
      }
    }
  },
  "require": {
    "twilio/sdk": "^5.38"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.4"
  }
}
