{
  "name": "i-rocky/laravel-twilio",
  "description": "Twilio Fax, SMS, Call notification support for laravel",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Rocky",
      "email": "wpkpda@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rocky\\LaravelTwilio\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rocky\\LaravelTwilio\\Providers\\TwilioServiceProvider"
      ],
      "aliases": {
        "LaravelTwilio": "Rocky\\LaravelTwilio\\Facade"
      }
    }
  },
  "require": {
    "twilio/sdk": "^5.38"
  }
}
